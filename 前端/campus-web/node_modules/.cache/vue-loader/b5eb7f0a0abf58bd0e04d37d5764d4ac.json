ntire\" label=\"全部\"></el-option>\n                    <el-option :value=\"item.id\" v-for=\"item in schools\" :label=\"item.name\"></el-option>\n                </el-select>\n                <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"clickSearch\"></el-button>\n            </el-input>\n        </div> -->\n        <div class=\"bottom\">\n            <el-table\n                    :data=\"tasks\"\n                    :resizable=\"false\"\n                    style=\"width: 100%  \">\n                <el-table-column\n                        prop=\"publish.username\"\n                        label=\"发布人\"\n                        min-width=\"140\">\n                </el-table-column>\n                <el-table-column\n                        label=\"接受人\"\n                        min-width=\"140\">\n                    <template slot-scope=\"scope\">\n                        {{scope.row.accept ? scope.row.accept.username : '暂无服务'}}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        prop=\"reward\"\n                        label=\"任务奖励\"\n                        min-width=\"110\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"school.name\"\n                        label=\"任务所在学校\"\n                        min-width=\"200\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"taskTitle\"\n                        label=\"标题\"\n                        min-width=\"180\">\n                </el-table-column>\n                <el-table-column\n                        label=\"发布时间\"\n                        min-width=\"160\">\n                    <template slot-scope=\"scope\">\n                        {{scope.row.createTime ? transform(scope.row.createTime) : '暂无时间'}}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        label=\"服务时间\"\n                        min-width=\"160\">\n                    <template slot-scope=\"scope\">\n                        {{scope.row.orderTime ? transform(scope.row.orderTime) : '暂无时间'}}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        prop=\"balance\"\n                        label=\"完成时间\"\n                        min-width=\"160\">\n                    <template slot-scope=\"scope\">\n                        {{scope.row.endTime ? transform(scope.row.endTime) : '暂无时间'}}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        label=\"任务状态\"\n                        min-width=\"90\">\n                    <template slot-scope=\"scope\">\n                        {{scope.row.state == 0?'未处理':scope.row.state == 1 ? '处理中' : '已完成'}}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        label=\"操作\"\n                        width=\"100\">\n                    <template slot-scope=\"scope\">\n                        <el-popconfirm\n                                confirm-button-text='好的'\n                                cancel-button-text='不用了'\n                                icon=\"el-icon-info\"\n                                icon-color=\"red\"\n                                title=\"确定删除该任务吗？\"\n                                @confirm=\"del(scope.row.id,scope.row.state)\"\n                        >\n                            <el-button type=\"text\" size=\"small\" slot=\"reference\"><i class=\"el-icon-delete\" style=\"color: red\"></i></el-button>\n                        </el-popconfirm>\n                    </template>\n                </el-table-column>\n            </el-table>\n\n        </div>\n    </div>\n</template>\n\n<script>\n    import {formatDate} from '@/util/date';\n    export default {\n        name: \"Task\",\n        methods: {\n            inquiry(){\n                if (this.select == 'entire'){\n                    this.newList();\n                }else {\n                    this.$post(\"/task/api/findTaskBySchoolId\",{\"id\":this.select})\n                    .then(res => {\n                        this.tasks = res.data.tasks\n                    })\n                }\n            },\n            clickSearch() {\n                if(this.input == '已完成'){\n\n                }\n            },\n\n            del(id,state) {\n                if (state == 0){\n                    this.$del(\"/task/\"+id)\n                    .then((res) => {\n                        this.$notifyMsg(\"成功\", res.data.msg, \"success\")\n                        this.newList()\n                    })\n                }else {\n                    this.$msg(\"该任务已被接收或已完成\",\"error\")\n                }\n            },\n\n            newList() {\n                this.$get(\"/task\")\n                .then((rs) => {\n                    this.tasks = rs.data.task\n                    // console.log(this.tasks)\n                })\n            },\n            transform(time) {\n                let date = new Date(time);\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\n            }\n        },\n        data() {\n            return {\n                //学校\n                schools:[],\n                tasks: [],\n                ruleForm: {\n                    id: 0,\n                    state: true\n                },\n                options: [\n                    {label: \"正常\", value: \"0\"},\n                    {label: \"禁用\", value: \"1\"}\n\n                ],\n                input:\"\",\n                select: 'entire'\n            }\n        },\n        created() {\n            this.newList();\n            this.$get(\"school\")\n            .then(res => {\n                this.schools = res.data.schools\n            })\n        },\n\n        filters: {\n            formatDate(time) {\n                let date = new Date(time);\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\n            }\n        }\n\n    }\n</script>\n\n<style scoped lang=\"less\">\n    .content {\n        padding: 0 1%;\n\n    }\n\n    .center {\n        width: 80%;\n        margin-bottom: 30px;\n    }\n\n    /deep/ .el-select .el-input {\n        width: 200px;\n    }\n\n    /deep/ .input-with-select .el-input-group__prepend {\n        background-color: #fff;\n    }\n\n\n\n    .form {\n        margin: 0 22px;\n    }\n</style>"]}]}